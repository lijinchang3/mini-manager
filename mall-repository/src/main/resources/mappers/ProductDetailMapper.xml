<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="me.liuhui.mall.repository.dao.ProductDetailDao">

	<select id="selectByProductId" resultType="me.liuhui.mall.repository.model.ProductDetail">
		select * from p_product_detail where product_id = #{productId}
	</select>


</mapper>

